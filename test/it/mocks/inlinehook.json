{
  "name": "Mock OIDC/OAuth token inline hook",
  "type": "com.okta.oauth2.tokens.transform",
  "version": "1.0.0",
  "channel": {
    "type": "HTTP",
    "version": "1.0.0",
    "config": {
      "uri": "https://example.com",
      "headers": [
        {
          "key": "x-any-key",
          "value": "my-header-value"
        }
      ],
      "method": "POST",
      "authScheme": {
        "type": "HEADER",
        "key": "Authorization",
        "value": "my-shared-secret"
      }
    }
  }
}