{
  "name": "@okta/okta-sdk-nodejs",
  "version": "0.3.1",
  "description": "Okta API wrapper for Node.js",
  "engines": {
    "node": ">=4.8.3"
  },
  "files": [
    "src/",
    "README.md"
  ],
  "main": "src/index.js",
  "scripts": {
    "banners": "./utils/maintain-banners.js",
    "build": "okta-sdk-generator -t templates/ -o . && ./utils/maintain-banners.js",
    "predocs": "rm -rf ./jsdocs",
    "docs": "./node_modules/.bin/jsdoc src/ -c ./docs/config.json -d ./jsdocs/ -P ./package.json -R ./docs/JSDOC.md -r",
    "test": "eslint -c src/.eslintrc src/ && eslint -c test/.eslintrc test/ && nyc mocha test/**/*.js --no-timeouts"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "repository": "https://github.com/okta/okta-sdk-nodejs",
  "dependencies": {
    "flat": "^2.0.1",
    "isomorphic-fetch": "2.2.1",
    "js-yaml": "^3.8.4",
    "lodash": "^4.17.4",
    "parse-link-header": "1.0.0",
    "strict-mode": "^1.1.2"
  },
  "devDependencies": {
    "@okta/openapi": "^0.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "fake-fs": "^0.5.0",
    "faker": "^4.1.0",
    "globby": "^6.1.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.0",
    "mocha": "^3.4.1",
    "nyc": "^10.3.2",
    "speakeasy": "^2.0.0"
  }
}
